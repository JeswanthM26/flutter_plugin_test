    }
  ]
},
"apz_peripherals": {
  "fields": [
    {
      "key": "operation",
      "label": "Select Operation",
      "type": "dropdown",
      "options": ["battery", "bluetooth", "nfc"],
      "default": "battery"
    }
  ]
},
"apz_screen_security": {
  "fields": [
    {
      "key": "operation",
      "label": "Select Operation",
      "type": "dropdown",
      "options": ["enable", "disable", "status"],
      "default": "status"
    }
  ]
},
"apz_share": {
  "fields": [
    {
      "key": "shareType",
      "label": "Share Type",
      "type": "dropdown",
      "options": ["text", "file", "multiple_files", "asset_file"],
      "default": "text"
    },
    {
      "key": "title",
      "label": "Title",
      "type": "text",
      "required": true
    },
    {
      "key": "text",
      "label": "Text",
      "type": "text",
      "required": false
    },
    {
      "key": "subject",
      "label": "Subject (for text)",
      "type": "text",
      "required": false,
      "dependsOn": {
        "key": "shareType",
        "value": "text"
      }
    },
    {
      "key": "filePath",
      "label": "File Path (for single file)",
      "type": "text",
      "required": false,
      "dependsOn": {
        "key": "shareType",
        "value": "file"
      }
    },
    {
      "key": "filePaths",
      "label": "File Paths (comma-separated)",
      "type": "text",
      "required": false,
      "dependsOn": {
        "key": "shareType",
        "value": "multiple_files"
      }
    },
    {
      "key": "assetPath",
      "label": "Asset Path",
      "type": "text",
      "required": false,
      "dependsOn": {
        "key": "shareType",
        "value": "asset_file"
      }
    },
    {
      "key": "mimeType",
      "label": "MIME Type (for asset file)",
      "type": "text",
      "default": "application/octet-stream",
      "required": false,
      "dependsOn": {
        "key": "shareType",
        "value": "asset_file"
      }
    }
  ]
},
"apz_webview": {
  "fields": [
    {
      "key": "url",
      "label": "Webview URL",
      "type": "text",
      "default": "https://example.com"
    },
    {
      "key": "usePost",
      "label": "Use POST Request?",
      "type": "switch",
      "default": false
    },
    {
      "key": "postData",
      "label": "POST Data (key1=value1&key2=value2)",
      "type": "text",
      "default": ""
    },
    {
      "key": "showAcceptReject",
      "label": "Show Accept/Decline Buttons?",
      "type": "switch",
      "default": false
    }
  ]
},
"apz_idle_timeout": {
  "fields": [
    {
      "key": "timeoutSeconds",
      "label": "Timeout Duration (seconds)",
      "type": "number",
      "default": 10
    },
    {
      "key": "triggerNow",
      "label": "Trigger Timeout Immediately?",
      "type": "switch",
      "default": false
    }
  ]
}

}




